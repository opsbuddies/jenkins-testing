pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                sh '''
			export b_name=dev
			export instance=deepak
			bash abc.sh
		'''
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
